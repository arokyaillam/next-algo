{
  "name": "@workspace/market-data",
  "version": "1.0.0",
  "description": "Market data package for Nifty options trading",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.89.0",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "upstox-js-sdk": "^2.19.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8",
    "react": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@types/lodash-es": "^4.17.12",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "typescript": "^5.9.2"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./hooks/*": {
      "types": "./src/hooks/*.ts",
      "default": "./src/hooks/*.ts"
    },
    "./services/*": {
      "types": "./src/services/*.ts",
      "default": "./src/services/*.ts"
    },
    "./stores/*": {
      "types": "./src/stores/*.ts",
      "default": "./src/stores/*.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "default": "./src/types/index.ts"
    },
    "./utils/*": {
      "types": "./src/utils/*.ts",
      "default": "./src/utils/*.ts"
    }
  }
}
